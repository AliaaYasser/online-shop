                                <li class="nav-item">
                                    @Html.ActionLink("Commerce", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                                </li>
                                <li class="nav-item">
                                    @Html.ActionLink("  Home  ", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                                </li>
                                <li class="nav-item">
